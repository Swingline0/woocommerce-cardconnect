{
  "name": "woocommerce-cardconnect-payment-gateway",
  "version": "3.1.0-beta",
  "description": "Accept credit card payments in your WooCommerce store!",
  "main": "index.js",
  "engines": {
    "node": "8"
  },
  "scripts": {
    "build": "gulp",
    "postinstall": "npm run build",
