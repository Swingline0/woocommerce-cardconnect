    "docker-compose": "^0.8.1",
    "gulp": "^3.9.1",
    "gulp-cli": "^2.0.1",
    "gulp-if": "^1.2.5",
    "gulp-notify": "^2.2.0",
    "gulp-plumber": "^1.2.0",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-typescript": "^5.0.0-alpha.3",
    "gulp-uglify": "^1.5.4",
    "mocha": "^2.2.1",
    "tsify": "^4.0.0",
    "typescript": "^2.9.2",
